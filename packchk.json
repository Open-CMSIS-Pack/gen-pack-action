{
  "problemMatcher": [
    {
      "owner": "packchk",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^\\*\\*\\* (INFO|WARNING|ERROR) M(\\d+): (.*) \\(Line (\\d+)\\)",
          "severity": 1,
          "file": 3,
          "line": 4,
          "code": 2
        },
        {
          "regexp": "^  (.+)",
          "message": 1,
          "loop": true
        }
      ]
    }
  ]
}
