{
  "problemMatcher": [
    {
      "owner": "packchk",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^\\*\\*\\* WARNING M(\\d+): (.*) \\(Line (\\d+)\\)",
          "file": 2,
          "line": 3,
          "code": 1
        },
        {
          "regexp": "^  (.+)",
          "message": 1,
          "loop": true
        }
      ]
    }
  ]
}
